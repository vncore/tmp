{
  "name": "vncore/core",
  "description": "Laravel core admin package for all system project",
  "keywords": ["core admin","laravel core"],
  "homepage": "https://vncore.net",
  "license": "MIT",
  "authors": [
      {
          "name": "VN core",
          "email": "vncore.net@gmail.net"
      }
  ],
  "require": {
      "php": "^8.2",
      "unisharp/laravel-filemanager": "^2.8",
      "spatie/laravel-pjax": "^2.4",
      "phpoffice/phpspreadsheet": "^2.0",
      "spatie/laravel-json-api-paginate": "^1.14",
      "spatie/db-dumper": "^3.4",
      "google/apiclient": "^2.15",
      "jenssegers/agent": "^2.6",
      "barryvdh/laravel-dompdf": "^v2.1",
      "laravel/framework": "^11.0",
      "laravel/helpers": "^1.7",
      "laravel/legacy-factories": "^1.4",
      "laravel/socialite": "^5.12",
      "laravel/sanctum": "^4.0",
      "laravel/tinker": "^2.9",
      "laravel/ui": "^4.5"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.6"
},

"scripts": {
    "post-install-cmd": [
          "@php -r \"file_exists('storage/vncore_not_run.txt') || touch('storage/vncore_not_run.txt');\""
    ],
    "pre-install-cmd": [
          "@php -r \"file_exists('storage/vncore_not_run.txt') || touch('storage/vncore_not_run.txt');\""
    ],
    "post-package-install": [
          "@php -r \"file_exists('storage/vncore_not_run.txt') || touch('storage/vncore_not_run.txt');\""
    ]
},
  "autoload": {
      "psr-4": {
         "Vncore\\Core\\": "src/"
      }
  },

  "extra": {
      "laravel": {
          "dont-discover": [
          ],
          "providers": [
            "Vncore\\Core\\VncoreServiceProvider"
          ],
          "aliases": {
            "Admin" : "Vncore\\Core\\Admin\\Admin"
          }
      }
  }
}
